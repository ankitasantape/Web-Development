<html>
    <head>
         <title>
             
             <%= title %>
         </title>
    </head>
    <body>
        <h1>Playground</h1>
        <!-- Loop -->
        <ul>
            <% for (let i = 1; i<6 ; i++ ){ %>
            <li>
                <%= i%>
            </li>
            <% } %>   
        </ul>
        <ul>
            <% for (let i = 1; i<6 ; i++ ){ %>
            <li>
                <%= "Hello"%>
            </li>
            
            <% } %>   
        </ul>
        <!-- Conditional Statement -->
        <% if (true) { %>

               <h1>It's true!</h1>

            <% } else { %>

                <p>It's false :(</p>

                <% } %>
    </body>
</html>